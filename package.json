    "build": "next build",
    "postbuild": "node -e \"const fs = require('fs'); const path = require('path'); if (fs.existsSync('.next/static') && fs.existsSync('.next/standalone')) { const staticDir = '.next/static'; const targetDir = '.next/standalone/.next/static'; if (!fs.existsSync('.next/standalone/.next')) { fs.mkdirSync('.next/standalone/.next', { recursive: true }); } if (fs.existsSync(targetDir)) { fs.rmSync(targetDir, { recursive: true, force: true }); } fs.cpSync(staticDir, targetDir, { recursive: true }); console.log('âœ“ Static files copied to standalone directory'); }\"",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
